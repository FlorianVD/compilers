{
	"name": "Compilers Lab 5",
	"image": "thomasfaingnaert/compilers:latest",
	"runArgs": [
		"--cap-add=SYS_PTRACE",
		"--security-opt",
		"seccomp=unconfined"
	],
	"extensions": [
		"ms-vscode.cpptools",
		"ms-vscode.cmake-tools"
	],
	"features": {
		"ghcr.io/devcontainers/features/sshd:1": {
			"version": "latest"
		}
	}
}
